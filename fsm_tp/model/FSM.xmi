<?xml version="1.0" encoding="UTF-8"?>
<fsm_tp:FSM
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:fsm_tp="http://www.example.org/fsm_tp"
    xsi:schemaLocation="http://www.example.org/fsm_tp fsm_tp.ecore">
  <state name="s1"/>
  <state name="s2"/>
  <state name="s3"
      isFinal="true"/>
  <transition
      name="t1"
      source="#//@initialstate"
      target="#//@state.0"
      trigger="on"/>
  <transition
      name="t2"
      source="#//@state.0"
      target="#//@state.1"
      trigger="open"/>
  <transition
      name="t3"
      source="#//@state.1"
      target="#//@state.0"
      trigger="close"/>
  <transition
      name="t4"
      source="#//@state.1"
      target="#//@state.2"
      trigger="off"/>
  <initialstate
      name="s0"/>
</fsm_tp:FSM>
